{
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/index.html" }
    ],
    "builds": [
      {
        "src": "build/web/index.html",
        "use": "@vercel/static"
      }
    ],
    "outputDirectory": "build/web"
  }
  